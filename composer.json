{
    "name": "jtgrimes/test-mail",
    "description": "A Laravel package for functionally testing email",
    "type": "library",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "J.T. Grimes",
            "email": "jtgrimes@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "5.*",
        "illuminate/mail": "5.*",
        "illuminate/filesystem": "5.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "JTGrimes\\TestMail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
